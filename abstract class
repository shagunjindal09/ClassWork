package shagun;

abstract class A{
abstract void m1();
	void m2() {
		System.out.println("m2 method ");
	}
}
class B extends A{
	void m1() {
System.out.println("this is the abstract method");		
	}
}

public class abstracttest {

	public static void main(String[] args) {
	B b = new B();
	b.m1();
	b.m2();

	}

}
